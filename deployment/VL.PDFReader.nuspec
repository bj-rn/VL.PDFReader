<package xmlns="http://schemas.microsoft.com/packaging/2011/10/nuspec.xsd">
  <metadata>
    <id>VL.PDFReader</id>
    <!-- For more about versions see: https://thegraybook.vvvv.org/reference/extending/publishing.html#version -->
    <version>0.1.0</version>
    <title>VL.PDFReader</title>
   
    <!-- Provide authors and projectUrl --> 
    <authors>bj-rn</authors>
    <owners>bj-rn</owners>
    <projectUrl>https://github.com/bj-rn/VL.PDFReader</projectUrl>
    <license type="expression">MIT</license>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>

    <!-- Provide description and tags-->
    <summary>A simple PDF-Reader for VL based on PDFium.</summary>
    <description>Load PDF pages as Skia images or Stride textures in vvvv.</description>
    <copyright>Copyright Â© 2024</copyright>
    <tags>VL, vvvv, PDF, PDFium, Reader, Skia, Image, Stride, Texture</tags>

    <readme>docs\README.md</readme>

    <dependencies>
      <dependency id="bblanchon.PDFiumV8.Win32" version="128.0.6611" />
    </dependencies>

  </metadata>
  <files>
    <file src="..\lib\**\*.dll" target="lib\" exclude="" />
    <file src="..\*.vl" target="" exclude="" />
    <file src="..\help\**" target="help\" exclude="" />
    <file src="..\README.md" target="docs\" />
  </files>
</package>
